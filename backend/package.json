{
  "name": "pressing-backend",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "nodemon --exec ts-node src/server.ts",
    "build": "tsc",
    "start": "node dist/src/server.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "db:init": "npx sequelize-cli init",
    "db:create": "npx sequelize-cli db:create",
    "db:drop": "npx sequelize-cli db:drop",
    "db:migrate": "npx sequelize-cli db:migrate --migrations-path migrations --models-path /models --seeders-path /seeders",
    "db:migrate:undo": "npx sequelize-cli db:migrate:undo --migrations-path src/migrations",
    "db:migrate:undo:all": "npx sequelize-cli db:migrate:undo:all --migrations-path /migrations",
    "db:seed": "npx sequelize-cli db:seed:all --seeders-path seeders",
    "db:seed:undo": "npx sequelize-cli db:seed:undo:all --seeders-path seeders",
    "db:reset": "npm run db:drop && npm run db:create && npm run db:migrate && npm run db:seed"
  },
  "dependencies": {
    "bcryptjs": "^3.0.2",
    "cloudinary": "^2.7.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "express-validator": "^7.2.1",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.1",
    "multer": "^2.0.2",
    "pg": "^8.16.3",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.37.7"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/morgan": "^1.9.10",
    "@types/multer": "^2.0.0",
    "@types/node": "^24.7.0",
    "nodemon": "^3.1.10",
    "sequelize-cli": "^6.6.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  }
}
